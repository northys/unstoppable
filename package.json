{
  "name": "unstoppable",
  "version": "1.0.0",
  "description": "Product data scraper for thomann.de and other websites",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "ts-node-esm -T src/main.ts",
    "start:prod": "node dist/main.js",
    "extract-categories": "ts-node-esm -T src/extract-categories.ts",
    "extract-categories:prod": "node dist/extract-categories.js",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "scraper",
    "crawlee",
    "thomann"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^14.0.0",
    "crawlee": "^3.14.0"
  },
  "devDependencies": {
    "@apify/tsconfig": "^0.1.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
